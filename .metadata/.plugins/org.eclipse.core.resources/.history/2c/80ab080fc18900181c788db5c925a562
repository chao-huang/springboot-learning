package com.sun.amr.vo;
import java.io.Serializable;
import java.util.List;
public class Dept implements Serializable {
private Integer did; //部门的编号
private String title;//部门的名称
private Integer sflag;//特殊标记
private List<Emp> allEmps;//部门下的员工
private List<Groups> allGroups;//部门对应的权限组信息
public Dept() {
}
public Dept(Integer did, String title, Integer sflag, List<Emp> allEmps, List<Groups> allGroups) {
	this.did = did;
	this.title = title;
	this.sflag = sflag;
	this.allEmps = allEmps;
	this.allGroups = allGroups;
}
public Integer getDid() {
	return did;
}
public void setDid(Integer did) {
	this.did = did;
}
public String getTitle() {
	return title;
}
public void setTitle(String title) {
	this.title = title;
}
public Integer getSflag() {
	return sflag;
}
public void setSflag(Integer sflag) {
	this.sflag = sflag;
}
public List<Emp> getAllEmps() {
	return allEmps;
}
public void setAllEmps(List<Emp> allEmps) {
	this.allEmps = allEmps;
}
public List<Groups> getAllGroups() {
	return allGroups;
}
public void setAllGroups(List<Groups> allGroups) {
	this.allGroups = allGroups;
}
@Override
public String toString() {
	return "Dept [did=" + did + ", title=" + title + ", sflag=" + sflag + ", allEmps=" + allEmps + ", allGroups="
			+ allGroups + "]";
}

}
